{
  "name": "momentia",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down --remove-orphans",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:seed": "prisma db seed",
    "prepare": "echo \"[prepare] prisma generate\" && prisma generate",
    "postinstall": "echo \"[postinstall] prisma generate\" && prisma generate",
    "stripe:listen": "stripe listen --forward-to http://localhost:3000/api/stripe/webhook",
    "stripe:trigger": "stripe trigger checkout.session.completed"
  },
  "dependencies": {
    "@azure/communication-email": "^1.0.0",
    "@azure/identity": "^4.10.1",
    "@azure/storage-blob": "^12.27.0",
    "@prisma/client": "^6.16.3",
    "date-fns": "^4.1.0",
    "exifr": "^7.1.3",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^5.0.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-photo-album": "^3.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.3",
    "stripe": "^18.5.0",
    "xss": "^1.0.15",
    "yet-another-react-lightbox": "^3.25.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/date-fns": "^2.5.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.16.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
